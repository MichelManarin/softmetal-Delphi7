  object SQLFornecedores: TSQLDataSet
    CommandText = 
      'SELECT'#13#10#13#10'FORNECEDORES.IDFORNECEDOR,'#13#10'FORNECEDORES.IDCEP_CEP,'#13#10'F' +
      'ORNECEDORES.NOMEFAN_FORN,'#13#10'FORNECEDORES.CPF_FORNE,'#13#10'FORNECEDORES' +
      '.RG_FORNE,'#13#10'FORNECEDORES.RS_FORN,'#13#10'FORNECEDORES.IE_FORN,'#13#10'FORNEC' +
      'EDORES.CNPJ_FORNE,'#13#10'FORNECEDORES.TEL_FORN,'#13#10'FORNECEDORES.CELULAR' +
      '_FORN,'#13#10'FORNECEDORES.FAX_FORN,'#13#10'FORNECEDORES.EMAIL_FORN,'#13#10'FORNEC' +
      'EDORES.CONTATO_FORN,'#13#10'FORNECEDORES.HOMEPAGE_FORN,'#13#10'FORNECEDORES.' +
      'COMPLE_FORN,'#13#10'FORNECEDORES.STATUS_FORN,'#13#10'FORNECEDORES.FOTO_FORN,' +
      #13#10#13#10'CIDADES.NOME_CID,'#13#10'CIDADES.IDCIDADE,'#13#10'BAIRROS.IDBAIRRO,'#13#10'BAI' +
      'RROS.NOME_BAIRRO,'#13#10'CEPS.LOG_CEP,'#13#10'CEPS.CEP_CEP,'#13#10'CEPS.IDCEP_CEP'#13 +
      #10#13#10'FROM FORNECEDORES'#13#10#13#10'LEFT OUTER JOIN CEPS ON (CEPS.IDCEP_CEP ' +
      '= FORNECEDORES.IDCEP_CEP)'#13#10'LEFT OUTER JOIN CIDADES ON ( CIDADES.' +
      'IDCIDADE = CEPS.IDCIDADE )'#13#10'LEFT OUTER JOIN BAIRROS ON (BAIRROS.' +
      'IDBAIRRO = CEPS.IDBAIRRO)'#13#10#13#10'WHERE FORNECEDORES.IDFORNECEDOR = :' +
      'PARFORNECEDOR'
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftString
        Name = 'PARFORNECEDOR'
        ParamType = ptInput
        Value = '0'
      end>
    SQLConnection = DM.conexao
